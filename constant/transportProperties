FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}

// -------------------------------------------------------------------------
// Transport properties for incompressible solvers (e.g. simpleFoam)
//
// rho  = density of the fluid [kg/m3]
// mu   = dynamic viscosity [Pa·s = kg/(m·s)]
// nu   = kinematic viscosity [m2/s], computed as mu / rho
//
// For Ahmed benchmark: rho = 1.2 kg/m3, mu = 1.8e-5 Pa·s → nu ≈ 1.5e-5 m2/s
// Adjust rho and mu if needed for different ambient conditions (FSAE work).
// -------------------------------------------------------------------------

// Fluid density (kg/m3)
rho     1.2;

// Dynamic viscosity (Pa·s)
mu      1.8e-5;

transportModel  Newtonian;

// Kinematic viscosity [m2/s] = mu / rho
nu      #eval "$mu/$rho";
